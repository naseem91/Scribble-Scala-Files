module Greeting;

type <java> "java.lang.String" from "rt.jar" as String;

global protocol Proto(role c, role s) {
    choice at c {
        Greet(String) from c to s;
  choice at s {
            Hello(String) from s to c;
      do Proto(c, s);
        } or {
            Bye(String) from s to c;
        }
    } or {
        Quit() from c to s;
    }
}
